    "globby": "^11.0.4",
    "gm": "^1.23.1",
    "html-minifier": "^4.0.0",
    "isomorphic-style-loader": "^5.1.0",
    "lazy-require": "^4.6.0",
    "stylelint": "^13.13.1",
    "svgo": "^2.3.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webfont": "^11.2.4",
    "webpack": "^5.41.1"
